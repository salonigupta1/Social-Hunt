<%= link_to "Create New Post", new_community_post_path(@community), class: "btn btn-primary float-end mt-5" %>


<%= @community.name %>

<% @posts.each do |post| %>
    <div class = "card p-2 mt-5" style="width: 50rem;">
        <h4><%= link_to post.title, community_post_path(@community, post) %></h4>
        <p><%= truncate post.body, length: 200 %></p>
     </div>
<% end %>



